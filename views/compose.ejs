<%-include("partials/header");-%>
<h1>Compose</h1>
<form action="/compose" method="post">
<div class="form-group">
  <label for="pTitle">Title</label>
  <input type="text" id="pTitle" class="form-control" name="postTitle" >
</div>
<div class="form-group">
  <label for="composedText">Post</label>
  <textarea name="composedText" id="composedText" class="form-control" rows="4" height="auto"></textarea>
</div>

  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%-include("partials/footer");-%>
